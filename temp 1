#include <DHT.h>
#include <DHT_U.h>
#include <Adafruit_Sensor.h>

#define DHT11_PIN 2

DHT t,h;

void setup(){
  Serial.begin(9600);
}

void loop()
{
  int chk = DHT.read11(DHT11_PIN);
  Serial.print("Temperature = ");
  Serial.println(t);
  Serial.print("Humidity = ");
  Serial.println(h);
  delay(1000);
}

